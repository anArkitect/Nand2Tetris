/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:

    Not(in = sel, out = notsel);

    And(a = a, b = notsel, out = u1);
    And(a = b, b = sel, out = u2);

    Or(a = u1, b = u2, out = out);
}